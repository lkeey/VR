CREATE TABLE game (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    description TEXT NOT NULL,
    image BLOB NOT NULL,
    video_path TEXT NOT NULL
);

insertGame:
INSERT INTO game(name, description, image, video_path)
VALUES (?, ?, ?, ?);

deleteGameById:
DELETE FROM game WHERE id = ?;

selectAllGames:
SELECT * FROM game;
